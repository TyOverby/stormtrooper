(set default-unit px)

(define quickline (lambda (x)
    (define dist (sqrt (+. (->float (* x x )) (->float (* 150 150)))))
    (cut-circle x 0 5)
    (cut-circle 0 150 dist)
    (cut-line x 0 0 150)
))

(quickline 0)
(quickline 50)
(quickline 100)
(quickline 200)

(cut-line
    100
    0
    0
    150)


(cut-line
    (px 0)
    (px 50)
    (px 150)
    (px 50))
